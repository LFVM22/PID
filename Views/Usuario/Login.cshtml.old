@using Models
@model Usuario
<link rel="stylesheet" href="../static/css/login.css">
<div class="login-page">
    
  <form class="form" method="POST">
    <span class="login-form">
      <input type="text" placeholder="username" asp-for="@Model.Login">
      <input type="password" placeholder="password" asp-for="@Model.Senha">
      <button type="submit">Login</button>
      <a asp-action="Cadastar">Cadastrar-se</a>
    </span>
  </form>
</div>